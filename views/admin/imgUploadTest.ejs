<head>
    <script src="https://unpkg.com/axios/dist/axios.min.js"></script>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-eOJMYsd53ii+scO/bJGFsiCZc+5NDVN2yr8+0RDqr0Ql0h+rP48ckxlpbzKgwra6" crossorigin="anonymous">
</head>
<body>
<form method="post">
    <input type="text" name="code" id="code" />
    <input type="file" name="thumbnail" id="thumbnail" />
    <input type="submit" onclick="save()"/>
</form>
<script>

    function save() {
        const frm = new FormData();
        const photoFile = document.getElementById("thumbnail");
        frm.append("code", document.getElementById("code"));
        frm.append("thumbnail", photoFile.files[0]);
        axios.post('http://localhost:4242/admin/product/register', frm, {
            headers: {
                'Content-Type': 'multipart/form-data'
            }
        })
            .then((response) => {
                console.log(response);
            })
            .catch((error) => {
                // 예외 처리
            })
        window.location.assign('/');
    }

</script>

</body>